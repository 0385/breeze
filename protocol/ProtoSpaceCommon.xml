<?xml version="1.0" encoding="UTF-8"?>

<ProtoTraits>
  <MinNo>10000</MinNo>
  <MaxNo>15000</MaxNo>
  <UseLog4z>1</UseLog4z>
</ProtoTraits>


<Proto>


     <!-- 场景类型 -->
    <enum type="ui16" name="SCENE_TYPE" desc="场景类型, 每个space都绑定一个场景类型" >
        <member name="SCENE_TYPE_NONE" value="0"   desc="无效"/>
        <member name="SCENE_TYPE_HOME"   desc="主城"/>
        <member name="SCENE_TYPE_SOME_INSTANCING"  desc="一些副本"/>
        <member name="SCENE_TYPE_ARENA" desc="竞技场"/>
        <member name="SCENE_TYPE_GUILD" desc="公会"/>
    </enum>


    <!-- 场景状态枚举-->
    <enum type="ui16" name="SCENE_STATUS" desc="场景状态, 是否在某个场景中">
        <member name="SCENE_STATUS_NONE" value="0" desc="不存在" />
        <member name="SCENE_STATUS_MATCHING" desc="正在匹配" />
        <member name="SCENE_STATUS_WAITING" desc="匹配成功,等待玩家进入" />
        <member name="SCENE_STATUS_INSTANCING" desc="场景中" />
    </enum>

    <!-- 场景信息-->
    <struct name="SceneInfo" desc="场景信息">
        <member name="type" type="ui16"  desc="类型"/>
        <member name="status" type="ui16"  desc="状态"/>
        <member name="host" type="string"  desc="服务器host"/>
        <member name="port" type="ui16"  desc="服务器port"/>
        <member name="spaceID" type="ui32"  desc="空间(场景,房间,战场,INSTANCING ID)的实例ID"/>
        <member name="token" type="SessionToken"  desc="令牌"/>
        <member name="isActive" type="ui8"  desc="当前活跃场景, 用于场景切换过渡,或者同时多个场景存在的情况"/>
    </struct>
    <array name="SceneInfoArray" type="SceneInfo" desc="" />




    <!------------------------------------------  实体 ------------------------------------------------------------------>
    <!-- 实体状态-->
    <enum type="ui16" name="ENTITY_STATE" desc="">
        <member name="ESTATE_FREEZING" value="0" desc="冻结, 不能移动,不能操作, 100%免伤" />
        <member name="ESTATE_ACTIVE" desc="活跃状态" />
        <member name="ESTATE_PRE_DIED" desc="进入死亡流程" />
        <member name="ESTATE_DIED" desc="死亡状态" />
    </enum>

    <!-- 实体对象的类型, 区别玩家, AI, 技能-->
    <enum type="ui16" name="ENTITY_TYPE" desc="">
        <member name="ETYPE_INVALID" value="0" desc="" />
        <member name="ETYPE_AVATAR" desc="" />
        <member name="ETYPE_AI" desc="" />
        <member name="ETYPE_FLIGHT" desc="飞行道具" />
    </enum>

    <!-- 实体阵营-->
    <enum type="ui16" name="ENTITY_COLOR" desc="">
        <member name="ECOLOR_INVALID" value="0" desc="红方" />
        <member name="ECOLOR_RED" desc="红方" />
        <member name="ECOLOR_BLUE" desc="蓝方" />
        <member name="ECOLOR_NEUTRAL" value="1000" desc="[0~ECOLOR_NEUTRAL)阵营相互敌对, [ECOLOR_NEUTRAL~)中立温和阵营" />
    </enum>

    <!-- 移动行为-->
    <enum type="ui16" name="MoveAction" desc="">
        <member name="MACTION_IDLE" value="0" desc="空闲" />
        <member name="MACTION_FACE"   desc="朝向"/>
        <member name="MACTION_FOLLOW" desc="跟随" />
        <member name="MACTION_PATH" desc="路径" />
    </enum>

    <!-- 搜索实体-->
    <enum type="ui16" name="SEARCH_TYPE" desc="">
        <member name="SEARCH_DISTANCE" value="1" desc="org 半径" />
        <member name="SEARCH_SEACTOR" value="2" desc="org 扇形" />
        <member name="SEARCH_RECT" value="3" desc="org 矩形" />
    </enum>

    <!-- 搜索目标类型,组合位结构-->
    <enum type="ui32" name="SEARCH_TARGET_TYPE" desc="">
        <member name="SEARCH_TARGET_SELF" value="1" desc="自身, 玩家或者AI" />
        <member name="SEARCH_TARGET_AVATAR" value="2" desc="施法者, 玩家或者AI" />
        <member name="SEARCH_TARGET_AI" value="3" desc="施法者, 玩家或者AI" />
        <member name="SEARCH_TARGET_BARRIER" value="4" desc="障碍" />
        <member name="SEARCH_TARGET_FLIGHT" value="5" desc="飞行道具" />
        <member name="SEARCH_TARGET_ENEMY" value="6" desc="敌人" />
        <member name="SEARCH_TARGET_FRIEND" value="7" desc="全体友方" />
        <member name="SEARCH_TARGET_NEUTRAL" value="8" desc="中立" />
    </enum>


    <!-- SKILL类型(组合位结构)  -->
    <enum type="ui64" name="SKILL_TYPE" desc="">
        <member name="SKILL_NORMAL_ATTACK"  value="1"  desc="普通攻击"/>
        <member name="SKILL_AUTO"    desc="被动技能"/>
        <member name="SKILL_CAN_BREAK"   desc="可被中断"/>
        <member name="SKILL_CAN_MOVE"   desc="可移动"/>
        <member name="SKILL_PHYSICAL"    desc="物理攻击"/>
        <member name="SKILL_MAGIC"    desc="魔法攻击"/>
        <member name="SKILL_HARM"   desc="进行伤害计算"/>
        <member name="SKILL_BREAK_SELF_MOVE"   desc="打断自己移动"/>
        <member name="SKILL_BREAK_SELF_SKILL"   desc="打断自己的技能"/>
        <member name="SKILL_BREAK_TARGET_MOVE"   desc="打断目标移动"/>
        <member name="SKILL_BREAK_TARGET_SKILL"   desc="打断目标技能"/>
        <member name="SKILL_BREAK_TARGET_DEBUFF"   desc="驱散目标减益BUFF"/>
        <member name="SKILL_BREAK_TARGET_BUFF"   desc="驱散目标增益BUFF"/>
    </enum>


    <!-- BUFF类型(组合位结构) 35以下可任意搭配, 如果多个类型组合需要避免value/effect字段的使用冲突-->
    <enum type="ui64" name="BUFF_TYPE" desc="">
        <member name="BUFF_HALO"  value="1"  desc="组合类型: 光环"/>
        <member name="BUFF_BUFF"  value="2"  desc="组合类型: 增益buff"/>
        <member name="BUFF_DEBUFF"  value="3"  desc="组合类型: 减益BUFF"/>
        <member name="BUFF_HIDE"  value="4"  desc="组合类型: 客户端不表现"/>
        <member name="BUFF_SNEAK"  value="5"  desc="组合类型: 潜行 不会被非己方阵营的任何AOE技能搜索到"/>

        <member name="BUFF_HOLD_MOVE"  value="15"  desc="控制: 禁止移动"/>
        <member name="BUFF_REVERSE_MOVE" value="16"  desc="控制: 移动反向"/>
        <member name="BUFF_SILENCE" value="17"  desc="控制: 沉默所有技能"/>
        <member name="BUFF_SILENCE_WITHOUT_NORMAL" value="18"  desc="控制: 沉默非普通攻击"/>
        <member name="BUFF_SILENCE_PHYSICAL" value="19"  desc="控制: 沉默物理攻击"/>
        <member name="BUFF_SILENCE_MAGIC" value="20"  desc="控制: 沉默魔法攻击"/>

        <member name="BUFF_IMMUNE_MOVE" value="25"  desc="免疫: 免疫被控 移动禁止"/>
        <member name="BUFF_IMMUNE_SILENCE" value="26"  desc="免疫: 免疫沉默"/>
        <member name="BUFF_IMMUNE_DEBUFF" value="27"  desc="免疫: 免疫DEBUFF"/>
        <member name="BUFF_IMMUNE_PHYSICAL" value="28"  desc="免疫: 物攻免疫"/>
        <member name="BUFF_IMMUNE_MAGIC" value="29"  desc="免疫: 法攻免疫"/>
        <member name="BUFF_IMMUNE_BREAK" value="30"  desc="免疫: 不可被技能驱散"/>


        <member name="BUFF_INC_DAMAGE" value="35"  desc="属性加成: 加增伤率 万分比(除以10000)  value1为基础概率, value2为成长概率 "/>
        <member name="BUFF_SUB_DAMAGE" value="36"  desc="属性加成: 减伤率 万分比(除以10000)  value1为基础概率, value2为成长概率 "/>
        <member name="BUFF_INC_SPEED" value="41"  desc="属性加成: 加速度  万分比(除以10000) value1为固定速度加成, value2为概率加成"/>
        <member name="BUFF_INC_SUCK_BLOOD" value="42"  desc="属性加成: 吸血率 万分比(除以10000)  value1为基础概率, value2为成长概率 "/>

        <member name="BUFF_LIGHT_LIFE" value="50"  desc="持续性触发: 治疗(生命), 点燃(生命)   value1为每秒固定值, value2为每秒恢复/扣除相对于自己最大血量的万分比(除以10000)"/>
        <member name="BUFF_LIGHT_RUNSKILL" value="51"  desc="持续性触发: 触发一个技能,  光环BUFF,   配置:value填写要触发的技能ID, value1为间隔时间"/>

    </enum>

    <array name="SkillIDArray" type="ui32" desc="技能ID数组" />
    <array name="BuffIDArray" type="ui32" desc="buff ID 数组" />

    <struct name="TargetSearch" desc="">
        <member name="searchType" type="ui16"  desc=""/>
        <member name="targetType" type="ui64"  desc=""/>
        <member name="distance" type="float"  desc="伤害距离"/>
        <member name="radian" type="float"  desc="弧度或者宽度"/>
        <member name="offsetX" type="float"  desc="坐标偏移量"/>
        <member name="offsetY" type="float"  desc="坐标偏移量"/>
        <member name="targetMaxCount" type="ui32"  desc="最大目标数"/>
    </struct>

    <struct name="TargetAddSkillBuff" desc="目标上技能和buff">
        <member name="search" type="TargetSearch"  desc=""/>
        <member name="skills" type="SkillIDArray"  desc=""/>
        <member name="buffs" type="BuffIDArray"  desc=""/>
    </struct>
    <array name="TargetAddSkillBuffArray" type="TargetAddSkillBuff" desc="" />

    <struct name="HitData" desc="技能">
        <member name="act" type="ui16"  desc="序列"/>
        <member name="rate" type="float"  desc="概率"/>
        <member name="buffID" type="ui32"  desc=""/>
        <member name="delay" type="ui32"  desc="序列延迟"/>
    </struct>
    <array name="HitDataArray" type="HitData" desc="" />

    <struct name="SkillData" desc="技能">
        <member name="skillID" type="ui32"  desc="skillID"/>
        <member name="skillType" type="ui64"  desc="SKILL_TYPE"/>
        <member name="cd" type="ui32"  desc=""/>
        <member name="hitActions" type="HitDataArray"  desc="动作触发序列"/>
        <member name="targetAddSkillBuffs" type="TargetAddSkillBuffArray"  desc="上技能或者buff, 每个动作触发一次"/>
        <member name="targetDamage" type="TargetSearch"  desc="触发伤害"/>
        <member name="teleportDistance" type="float"  desc="瞬移"/>
   </struct>

    <struct name="BuffData" desc="buff">
        <member name="buffID" type="ui32"  desc=""/>
        <member name="piletime" type="ui32"  desc="最大叠加时间"/>
        <member name="keepTime" type="ui32"  desc="保持时间"/>
        <member name="buffType" type="ui64"  desc="buff类型"/>
        <member name="value1" type="i32"  desc="值1"/>
        <member name="value2" type="i32"  desc="值2"/>
   </struct>

    <!-- 伤害类型-->
    <enum type="ui16" name="HARM_TYPE" desc="已兼容客户端. 伤害类型">
        <member name="HARMTYPE_GENERAL" value="0" desc="普通伤害" />
        <member name="HARMTYPE_MISS" value="1" desc="闪避" />
        <member name="HARMTYPE_CRITICAL" value="2" desc="暴击" />
        <member name="HARMTYPE_HILL" value="3" desc="治疗" />
    </enum>

    <struct name="HarmData" desc="伤害数据">
        <member name="eid" type="ui32"  desc="目标eid"/>
        <member name="harm" type="float"  desc="如果为正是伤害, 为负则是回血"/>
        <member name="type" type="ui16"  desc="伤害类型HARM_TYPE"/>
   </struct>
   <array name="HarmDataArray" type="HarmData" desc="" />
   
    <!-- SkillInfo -->
    <struct name="SkillInfo" desc="" >
        <member name="skillID" type="ui32" />
        <member name="start" type="ui32" desc="start (server)tick"/>
        <member name="lastHitTick" type="ui32" desc="lastHitTick"/>
        <member name="seq" type="ui32" desc="hit seq"/>
        <member name="dst"  type="EPosition" desc="目标位置,只有需要用到的这个参数的技能才会读这个字段"/>  
        <member name="foe"  type="ui32" desc="锁定的目标"/>  
        <member name="data" type="SkillData" desc="配置数据"/>
    </struct>
    <array name="SkillInfoArray" type="SkillInfo" desc="" />


    <!-- BuffInfo -->
    <struct name="BuffInfo" desc="" >
        <member name="eid" type="ui32" desc="施放该buff的entity id"/>
        <member name="skillID" type="ui32" desc="如果该buff是被技能触发的 则记录该技能, 被动技能是0"/>
        <member name="buffID" type="ui32" />
        <member name="start" type="ui32" desc="start (server)tick"/>
        <member name="lastTrigerTick" type="ui32" desc="lastTrigerTick"/>
        <member name="data" type="BuffData" desc="配置数据"/>
    </struct>
    <array name="BuffInfoArray" type="BuffInfo" desc="" />




    <!-- 字典属性 -->
    <struct name="EntityDict" desc="字典属性">
        <member name="serviceID" type="ui64"  desc="用户ID, 非用户为InvalidServiceID"/>
        <member name="base" type="UserBaseInfo"  desc=""/>
        <member name="fight" type="DictFightEffect"  desc="战斗属性"/>
    </struct>
    <array name="EntityDictArray" type="EntityDict" desc="" />


    <!-- 实体坐标 -->
    <struct name="EPosition" desc="" >
        <member name="x" type="double" />
        <member name="y" type="double" />
        <member name="face" type="double" />
    </struct>
    <array name="EPositions" type="EPosition" desc="" />



    <!-- 实体动态属性, 差异广播 -->
    <struct name="EntityInfo" desc="EntityInfo">
        <member name="eid" type="ui32"  desc="eid"/>
        <member name="color" type="ui16"  desc="阵营"/>
        <member name="state" type="ui16"  desc="状态"/>
        <member name="pos"  type="EPosition" desc="当前坐标"/>
        <member name="moveAction" type="ui16"  desc="状态"/>
        <member name="movePath"  type="EPoints" desc="当前的移动路径"/>
        <member name="foe"  type="ui32" desc="锁定的敌人"/>
        <member name="leader"  type="ui32" desc="实体的老大, 如果是飞行道具 这个指向施放飞行道具的人"/>
        <member name="follow"  type="ui32" desc="移动跟随的实体"/>
        <member name="curHP"  type="double" desc="当前的血量"/>

    </struct>
    <array name="EntityInfoArray" type="EntityInfo" desc="" />

    <!-- 实体控制属性, 对客户端不可见 -->
    <struct name="EntityControl" desc="EntityControl">
        <member name="eid" type="ui32"  desc="eid"/>
        <member name="stateChageTick" type="ui32"  desc="状态改变时间"/>
        <member name="extSpeed" type="double"  desc="扩展速度"/>
        <member name="extBeginTick" type="ui32"  desc="扩展速度的开始时间"/>
        <member name="extKeepTick" type="ui32"  desc="扩展速度的保持时间"/>
        <member name="spawnpoint" type="EPosition"   desc="出生点"/>
        <member name="lastPos"  type="EPoint" desc="上一帧实体坐标, 如果是瞬移 则和pos相同"/>
        <member name="skills"  type="SkillInfoArray" desc="技能数据"/>
        <member name="buffs"  type="BuffInfoArray" desc="BUFF数据, 小标ID对应bufftype"/>
        <member name="diedTick"  type="ui32" desc="实体死亡时间点 -1为永久, 仅飞行道具类有效"/>
        <member name="hitTimes"  type="i32" desc="实体碰撞 -1为永久, 仅飞行道具类有效"/>
        <member name="lastMoveTick"  type="ui32" desc="最后一次移动时间"/>
        <member name="lastClientPos"  type="EPosition" desc="最后一次客户端提交的坐标"/>
    </struct>
    <array name="EntityControlArray" type="EntityControl" desc="" />

    <!-- 实体战报 -->
    <struct name="EntityReport" desc="EntityReport">
        <member name="eid" type="ui32"  desc="eid"/>
        <member name="killCount" type="ui32"  desc="杀死其他玩家次数"/>
        <member name="killTick" type="ui32"  desc="杀死其他玩家的时间"/>
        <member name="diedCount" type="ui32"  desc="死亡次数"/>
        <member name="topMultiKills" type="ui32"  desc="最高连杀次数"/>
        <member name="curMultiKills" type="ui32"  desc="当前连杀次数"/>
    </struct>
    <array name="EntityReportArray" type="EntityReport" desc="" />

    <!-- 一个实体的完整数据, 包括属性,状态 位置等. -->
    <struct name="EntityFullInfo" desc="EntityFullInfo">
        <member name="dict" type="EntityDict"  desc=""/>
        <member name="info" type="EntityInfo"  desc=""/>
        <member name="report" type="EntityReport"  desc=""/>
    </struct>
    <array name="EntityFullInfoArray" type="EntityFullInfo" desc="" />



</Proto>

