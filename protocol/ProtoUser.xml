<?xml version="1.0" encoding="UTF-8"?>

<ProtoTraits>
  <MinNo>40000</MinNo>
  <MaxNo>50000</MaxNo>
  <UseLog4z>true</UseLog4z>
</ProtoTraits>


<Proto>

    <packet name="ClientPulse" desc= "客户端脉冲请求">
    </packet>

    <packet name="ClientAuthReq" desc= "认证请求">
        <member name="account" type="string"   desc=""/>
        <member name="token" type="string"   desc=""/>
    </packet>
     <packet name="ClientAuthResp" desc= "认证返回">
        <member name="retCode" type="ui16"   desc=""/>
        <member name="account" type="string"   desc=""/>
        <member name="token" type="string"   desc=""/>
        <member name="previews" type="UserPreviewArray"   desc=""/>
    </packet>

    <packet name="CreateUserReq" desc= "创角色请求">
        <member name="accountName" type="string"   desc="这个字段会被服务器填充.客户端可以不填写"/>
        <member name="userName" type="string"   desc=""/>
    </packet>
     <packet name="CreateUserResp" desc= "创角色请求返回">
        <member name="retCode" type="ui16"   desc=""/>
        <member name="userID" type="ui64"   desc=""/>
        <member name="previews" type="UserPreviewArray"   desc=""/>
    </packet>

    <packet name="AttachUserReq" desc= "选角色请求">
        <member name="accountName" type="string"   desc="这个字段会被服务器填充.客户端可以不填写"/>
        <member name="userID" type="ui64"   desc=""/>
    </packet>
     <packet name="AttachUserResp" desc= "选角色请求返回">
        <member name="retCode" type="ui16"   desc=""/>
        <member name="userID" type="ui64"   desc=""/>
    </packet>


 

<!------------------------------------ battle --------------------------------------------->
   <packet name="GetSpaceInfoReq" desc= "">
    </packet>
    <packet name="GetSpaceInfoResp" desc= "">
        <member name="spaceInfo" type="SpaceTokenInfo"   desc=""/>
    </packet>

   <packet name="JoinSpaceReq" desc= "">
        <member name="spaceType" type="ui16"   desc=""/>
        <member name="mapID" type="ui32"   desc=""/>
    </packet>
    <packet name="JoinSpaceResp" desc= "">
        <member name="retCode" type="ui16"   desc=""/>
        <member name="spaceInfo" type="SpaceTokenInfo"   desc=""/>

    </packet>
    <packet name="JoinSpaceNotice" desc= "">
        <member name="spaceInfo" type="SpaceTokenInfo"   desc=""/>
    </packet>

   <packet name="LeaveSpaceReq" desc= "">
        <member name="spaceID" type="ui32"   desc=""/>
    </packet>
    <packet name="LeaveSpaceResp" desc= "">
        <member name="retCode" type="ui16"   desc=""/>
        <member name="spaceInfo" type="SpaceTokenInfo"   desc=""/>
    </packet>


<!------------------------------------ other --------------------------------------------->

   <packet name="UserChatReq" desc= "">
        <member name="userID" type="ui64"   desc=""/>
        <member name="msg" type="string"   desc=""/>
    </packet>
     <packet name="UserChatResp" desc= "">
        <member name="fromUserID" type="ui64"   desc=""/>
        <member name="msg" type="string"   desc=""/>
    </packet>

    <packet name="UserPingPongReq" desc= "">
        <member name="msg" type="string"   desc=""/>
    </packet>
     <packet name="UserPingPongResp" desc= "">
        <member name="msg" type="string"   desc=""/>
    </packet>




</Proto>

